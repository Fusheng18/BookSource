{
  "name": "亲小说",
  "url": "api.qinxiaoshuo.com",
  "version": 101,
  "search": {
      "url": "http://api.qinxiaoshuo.com/api/public/get_gems_by_keyword/小说/${key}/0",
      "charset": "UTF-8",
      "list": "$.gems[*]",
      "name": "$..gem_name",
      "cover": "http://static.qinxiaoshuo.com:4000/bookimg/${$.gem_id}.jpg",
      "summary": "$..gem_intro",
      "detail": "https://api.qinxiaoshuo.com/api/public/get_gem_detail/${$.gem_id}"
  },
  "detail": {
    "name": "$..gem_name",
    "author": "$..author_name",
    "cover": "http://static.qinxiaoshuo.com:4000/bookimg/${$.ore_id}.jpg",
    "summary": "$..gem_intro",
    "status": "$..status",
    "update": "$..last_update_time",
    "lastChapter": "$..last_update_content_name"
  },
  "catalog": {
      "list": "$..jewels[*]",
      "orderBy": 0,
      "booklet": {
        "name": "$..jewel_name",
        "list": "$..contents[*]"
    },
    "name": "$..content_name",
    "chapter": "https://api.qinxiaoshuo.com/api/public/get_content/${$.branch_id}/${$.content_id}"
  },
  "chapter": {
      "content": "$.content"
  },
  "rank": [
    {
        "title":"日轻",
        "url": "http://api.qinxiaoshuo.com/api/public/get_gems_by_tag/小说/1/${page}",
        "page": 1,
        "size": 10,
        "list": "$.gems[*]",
        "name": "$..gem_name",
        "cover": "http://static.qinxiaoshuo.com:4000/bookimg/${$.gem_id}.jpg",
        "detail": "https://api.qinxiaoshuo.com/api/public/get_gem_detail/${$.gem_id}"
    },
    {
      "title":"爱情",
      "url": "http://api.qinxiaoshuo.com/api/public/get_gems_by_tag/小说/2/${page}",
      "page": 1,
      "size": 10,
      "list": "$.gems[*]",
      "name": "$..gem_name",
      "cover": "http://static.qinxiaoshuo.com:4000/bookimg/${$.gem_id}.jpg",
      "detail": "https://api.qinxiaoshuo.com/api/public/get_gem_detail/${$.gem_id}"
    }
  ]
}
